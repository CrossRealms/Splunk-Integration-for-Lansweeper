[proxy]
proxy_enabled = <boolean> Proxy enabled/disabled
proxy_type = <string> Proxy type (http|socks4|socks5)
proxy_url = <string> Proxy URL
proxy_port = <string> Proxy port
proxy_username = <string> Proxy username
proxy_password = <string> Proxy password (encrypted)
proxy_rdns = <boolean> Remote DNS resolution

[logging]
loglevel = <string> DEBUG|INFO|WARNING|ERROR|CRITICAL
